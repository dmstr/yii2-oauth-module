{
    "name": "dmstr/yii2-oauth-module",
    "type": "library",
    "description": "Yii2 OAuth2 Module",
    "keywords": [
        "yii2",
        "oauth",
        "oauth2"
    ],
    "homepage": "https://www.dmstr.io/",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phan/phan": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "dmstr\\oauth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dmstr\\oauth\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "config": {
        "sort-packages": true
    }
}
